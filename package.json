{
  "name": "@dikac/tn-filesystem",
  "version": "0.1.1",
  "description": "typescript node file system",
  "dependencies": {
    "@dikac/deployer": "^0.1.5",
    "@dikac/t-function": "^0.7.0",
    "@types/node": "^14.0.23"
  },
  "devDependencies": {
    "jasmine-ts-console-reporter": "^3.1.1",
    "@types/jasmine": "^3.5.11",
    "cpy-cli": "^3.1.1",
    "jasmine": "^3.4.0",
    "json-properties-remover": "^0.1.2",
    "move-cli": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.6",
    "cpx": "^1.5.0"
  },
  "scripts": {
    "spec-run": "jasmine --reporter=jasmine-ts-console-reporter --config=./jasmine.json",
    "spec": "npm-run-all compile spec-compile spec-run",
    "spec-compile": "npm-run-all --parallel spec-compile-script spec-asset",
    "spec-compile-watch": "npm-run-all --parallel spec-compile-watch-script spec-asset-watch",
    "spec-compile-script": "tsc -p tsconfig-spec.json",
    "spec-compile-watch-script": "tsc -w -p tsconfig-spec.json",
    "compile": "tsc",
    "compile-watch": "tsc -w",
    "release": "remove-json-properties dist/package.json private scripts.postinstall scripts.deploy dependencies.@dikac/deployer",
    "postrelease": "npm publish dist",
    "prerelease": "cpy     package.json package-lock.json LICENSE Readme.md .npmignore dist",
    "postpublish": "rimraf package.json package-lock.json LICENSE Readme.md .npmignore",
    "postinstall": "npm run deploy",
    "deploy": "deployer /.git /dist /",
    "spec-asset-watch": "cpx \"spec/**/*.{json-example,json}\" dist-spec/ --watch --verbose",
    "spec-asset": "cpx \"spec/**/*.{json-example,json}\" dist-spec/ --verbose"
  },
  "private": true,
  "author": "",
  "license": "MIT"
}
